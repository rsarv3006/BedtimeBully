+import Foundation
+import GRDB
+
+public enum BedtimeScheduleTemplateServiceError: Error {
+    case bedtimeScheduleTemplateNotFound
+    case notificationScheduleNotFound
+}
+
+extension AppDatabase {
+    func buildDefaultBedtimeScheduleTemplate() throws {
+        try dbWriter.read { db in
+            if try GRDBBedtimeScheduleTemplate.all().isEmpty(db) {
+                guard let defaultNotificationScheduleId = try getFirstActiveNotificationSchedule()?.id else {
+                    throw BedtimeScheduleTemplateServiceError.notificationScheduleNotFound
+                }
+
+                let defaultBedtimeScheduleTemplate = try GRDBBedtimeScheduleTemplate.newDefaultBedtimeScheduleTemplate(notificationScheduleId: defaultNotificationScheduleId)
+                try defaultBedtimeScheduleTemplate.insert(db)
+            }
+        }
+    }
+
+    func updateBedtimeScheduleTemplateBedtimes(bedtimeScheduleTemplateId: UUID,
+                                               monday: BedtimeScheduleDay,
+                                               tuesday: BedtimeScheduleDay,
+                                               wednesday: BedtimeScheduleDay,
+                                               thursday: BedtimeScheduleDay,
+                                               friday: BedtimeScheduleDay,
+                                               saturday: BedtimeScheduleDay,
+                                               sunday: BedtimeScheduleDay) throws
+    {
+        try dbWriter.write { db in
+            guard var bedtimeScheduleTemplate = try GRDBBedtimeScheduleTemplate.fetchOne(db, key: bedtimeScheduleTemplateId) else {
+                throw BedtimeScheduleTemplateServiceError.bedtimeScheduleTemplateNotFound
+            }
+
+            bedtimeScheduleTemplate.monday = monday
+            bedtimeScheduleTemplate.tuesday = tuesday
+            bedtimeScheduleTemplate.wednesday = wednesday
+            bedtimeScheduleTemplate.thursday = thursday
+            bedtimeScheduleTemplate.friday = friday
+            bedtimeScheduleTemplate.saturday = saturday
+            bedtimeScheduleTemplate.sunday = sunday
+            try bedtimeScheduleTemplate.update(db)
+        }
+    }
+}
