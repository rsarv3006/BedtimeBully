+import Foundation
+import GRDB
+
+public extension AppDatabase {
+    func getFirstActiveNotificationSchedule() throws -> GRDBNotificationSchedule? {
+            try dbWriter.read { db in
+                try GRDBNotificationSchedule.all().getActiveSchedules().fetchOne(db)
+            }
+        }
+
+    func buildNotificationSchedules() throws {
+        try dbWriter.write { db in
+            if try GRDBNotificationSchedule.all().isEmpty(db){
+                let defaultNotificationSchedule = GRDBNotificationSchedule.new(name: "Default", notificationIntervals: [
+                    TimeInterval(30 * 60),
+                    TimeInterval(15 * 60),
+                    TimeInterval(10 * 60),
+                    TimeInterval(5 * 60),
+                    TimeInterval(3 * 60),
+                    TimeInterval(2 * 60),
+                    TimeInterval(1 * 60),
+                    TimeInterval(-1),
+                ], notificationMessages: [
+                    "Bedtime in 30 minutes",
+                    "Bedtime in 15 minutes",
+                    "Bedtime in 10 minutes",
+                    "Bedtime in 5 minutes",
+                    "Bedtime in 3 minutes",
+                    "Bedtime in 2 minutes",
+                    "Bedtime in 1 minute",
+                    "Time for Bed!",
+                ])
+                try defaultNotificationSchedule.insert(db)
+            }
+        }
+    }
+}
